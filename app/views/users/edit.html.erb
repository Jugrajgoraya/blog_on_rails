<h1>Sign Up</h1>
<% if @user.errors.any? %>
    <p><%= @user.errors.full_messages.join(', ') %> </p>
<% end %>

<%= form_with model: @user do |f|%>
    <div>
        <%= f.label :name%>
        <%= f.text_field :fname%>
    </div>
    <div>
        <%= f.label :email%>
        <%= f.text_field :email%>
    </div>
    <%= f.submit "Update"%>
<% end %>

